        	<Component Name="HPVOL" Type="ANALOG_MIXER_SWITCH_TYPE" Mapping="Amend3:HPVOL"/>
		<BooleanParameter Name="GAIN" Mapping="Control:'%1 %2 Gain'"/>
        </ComponentType>        
