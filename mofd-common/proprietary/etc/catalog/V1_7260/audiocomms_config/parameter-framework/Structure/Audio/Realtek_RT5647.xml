        </ComponentType>
        <ComponentType Name="HEADPHONE_MIXER_GAIN_TYPE">
            <Component Name="DAC1"  Type="ANALOG_MIXER_GAIN_TYPE" Mapping="Amend2:DAC1"/>
            <Component Name="DAC2"  Type="ANALOG_MIXER_GAIN_TYPE" Mapping="Amend2:DAC2"/>
            <Component Name="IN"  Type="ANALOG_MIXER_GAIN_TYPE" Mapping="Amend2:IN"/>
