        </ComponentType>
        <ComponentType Name="ANALOG_MIXER_VOL_CHANNEL_TYPE">
						<Component Name="LEFT" Type="ANALOG_MIXER_CHANNEL_SWITCH_TYPE" Mapping="Amend4:L"/>
						<Component Name="RIGHT" Type="ANALOG_MIXER_CHANNEL_SWITCH_TYPE" Mapping="Amend4:R"/>
        </ComponentType> 
